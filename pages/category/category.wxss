.container {
  padding: 10rpx;
}
.list-title {
  font-size: 40rpx;
  color: green;
  margin-bottom: 20rpx;
}
.category-list {
  display: flex;
  flex-wrap: wrap;
}
.category-list .item {
  flex: 0 0 33%;
  text-align: center;
  justify-content: center;
  display: flex;
  flex-direction: column;
  margin-bottom: 20rpx;
}
.category-list .item image{
  width: 80rpx;
  height: 50rpx;
  margin: 0 auto;
}